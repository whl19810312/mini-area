# Mini Area Server 환경 변수

# 서버 설정
NODE_ENV=development
PORT=7000
HOST=0.0.0.0

# 데이터베이스 설정
DB_HOST=localhost
DB_PORT=5432
DB_NAME=miniarea_db
DB_USER=miniarea_user
DB_PASSWORD=miniarea_pass
DATABASE_URL=postgresql://miniarea_user:miniarea_pass@localhost:5432/miniarea_db

# Redis 설정
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# JWT 설정
JWT_SECRET=miniarea-jwt-secret-key-2024
JWT_EXPIRES_IN=24h

# Agora 설정 (실제 값으로 교체 필요)
AGORA_APP_ID=your_agora_app_id
AGORA_APP_CERTIFICATE=your_agora_app_certificate

# CORS 설정
CORS_ORIGIN=https://localhost:5173,https://192.168.200.106:5173,http://localhost:3000

# SSL 설정
SSL_KEY_PATH=./ssl/key.pem
SSL_CERT_PATH=./ssl/cert.pem